@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix acl:   <http://www.w3.org/ns/auth/acl#> .
@prefix drumbeat: <http://drumbeat.cs.hut.fi/owl/lbdho.ttl#> .
@prefix ds:    <https://drumbeat.cs.hut.fi/owl/security.ttl#> .

ds:hasClub  a        owl:ObjectProperty ;
        rdfs:domain  rdfs:Resource ;
        rdfs:range   ds:Club .

ds:hasSubcontractor  a  owl:ObjectProperty ;
        rdfs:domain  ds:Contractor ;
        rdfs:range   ds:Contractor .

ds:Write  a     ds:Permission .

ds:hasRole  a        owl:ObjectProperty ;
        rdfs:domain  ds:AccessControlRule ;
        rdfs:range   ds:Role .

ds:Project  a            owl:Class ;
        rdfs:subClassOf  ds:Club .

ds:hasPermissionStatus
        a            owl:ObjectProperty ;
        rdfs:domain  ds:SecurityResponse ;
        rdfs:range   ds:PermissionStatus .

drumbeat:DataSource  a  owl:Class .

ds:hasTimeStamp  a   owl:DatatypeProperty ;
        rdfs:domain  ds:SecurityMessage ;
        rdfs:range   xsd:dateTime .

ds:Permission  a  owl:Class .

ds:Status  a    owl:Class .

ds:Contractor  a  owl:Class .

ds:hasName  a        owl:DatatypeProperty ;
        rdfs:domain  ds:Role ;
        rdfs:range   xsd:string .

ds:hasPermission  a  owl:ObjectProperty ;
        rdfs:domain  ds:AccessControlRule ;
        rdfs:range   ds:Permission .

ds:DENIED  a    ds:Status .

ds:trusts  a         owl:ObjectProperty ;
        rdfs:domain  ds:Contractor ;
        rdfs:range   <http://xmlns.com/foaf/0.1/Person> .

ds:hasContractor  a  owl:ObjectProperty ;
        rdfs:domain  ds:Club ;
        rdfs:range   ds:Contractor .

ds:Club  a      owl:Class .

ds:first  a          owl:ObjectProperty ;
        rdfs:domain  ds:ListNode .

ds:Delete  a    ds:Permission .

ds:Role  a      owl:Class .

drumbeat:DataSet  a  owl:Class .

ds:ListNode  a  owl:Class .

ds:hasWebID  a       owl:ObjectProperty ;
        rdfs:domain  ds:SecurityQuery ;
        rdfs:range   <http://xmlns.com/foaf/0.1/Person> .

ds:SecurityResponse  a   owl:Class ;
        rdfs:subClassOf  ds:SecurityMessage .

ds:hasRolePath  a    owl:ObjectProperty ;
        rdfs:domain  ds:Role ;
        rdfs:range   ds:ListNode .

ds:hasDataSource  a  owl:ObjectProperty ;
        rdfs:domain  drumbeat:Collection ;
        rdfs:range   drumbeat:DataSource .

ds:hasCollection  a  owl:ObjectProperty ;
        rdfs:domain  ds:DataSpaceNode ;
        rdfs:range   drumbeat:Collection .

ds:SecurityQuery  a      owl:Class ;
        rdfs:subClassOf  ds:SecurityMessage .

ds:Append  a    ds:Permission .

ds:hasProject  a            owl:ObjectProperty ;
        rdfs:domain         rdfs:Resource ;
        rdfs:range          ds:Project ;
        rdfs:subPropertyOf  ds:hasClub .

ds:hasDataSet  a     owl:ObjectProperty ;
        rdfs:domain  drumbeat:DataSource ;
        rdfs:range   drumbeat:DataSet .

ds:ACCEPTED  a  ds:Status .

ds:rest  a           owl:ObjectProperty ;
        rdfs:domain  ds:ListNode ;
        rdfs:range   ds:ListNode .

[ a                  owl:Restriction ;
  owl:allValuesFrom  rdf:Property ;
  owl:onProperty     ds:first
] .

ds:hasMainContractor  a     owl:ObjectProperty ;
        rdfs:domain         ds:Club ;
        rdfs:range          ds:Contractor ;
        rdfs:subPropertyOf  ds:hasContractor .

<http://xmlns.com/foaf/0.1/Person>
        a       owl:Class .

ds:hasMessage  a     owl:DatatypeProperty ;
        rdfs:domain  ds:SecurityResponse ;
        rdfs:range   xsd:string .

ds:DataSpaceNode  a  owl:Class .

drumbeat:Collection  a  owl:Class .

ds:hasAccessControlRule
        a            owl:ObjectProperty ;
        rdfs:domain  rdfs:Resource ;
        rdfs:range   ds:AccessControlRule .

ds:SecurityMessage  a  owl:Class .

ds:AccessControlRule  a  owl:Class .

ds:PermissionStatus  a  owl:Class ;
        owl:oneOf  ( ds:DENIED ds:ACCEPTED ) .

ds:Read  a      ds:Permission .
