
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ds:    <https://drumbeat.cs.hut.fi/owl/security.ttl#> .

ds:AuthorizationRule  a  owl:Class .

ds:first  a          owl:ObjectProperty ;
        rdfs:domain  ds:ListNode ;
        rdfs:range   rdf:Property .

ds:ProtectedResource  a  owl:Class .

ds:READ  a      ds:PermissionRole .

ds:RulePath  a  owl:Class .

ds:UPDATE  a    ds:PermissionRole .

ds:rest  a           owl:ObjectProperty ;
        rdfs:domain  ds:ListNode , ds:RulePath ;
        rdfs:range   ds:ListNode .

ds:DELETE  a    ds:PermissionRole .

ds:ListNode  a  owl:Class .

ds:hasClub  a        owl:ObjectProperty ;
        rdfs:domain  ds:ProtectedResource ;
        rdfs:range   ds:Club .

ds:hasProject  a            owl:ObjectProperty ;
        rdfs:domain         ds:ProtectedResource ;
        rdfs:range          ds:Project ;
        rdfs:subPropertyOf  ds:hasClub .

ds:PermissionRole  a  owl:Class .

ds:Project  a            owl:Class ;
        rdfs:subClassOf  ds:Club .

ds:CREATE  a    ds:PermissionRole .

ds:Club  a      owl:Class .

ds:hasAuthorization  a  owl:ObjectProperty ;
        rdfs:domain  ds:ProtectedResource ;
        rdfs:range   ds:AuthorizationRule .

ds:hasPermittedRole  a  owl:ObjectProperty ;
        rdfs:domain  ds:AuthorizationRule ;
        rdfs:range   ds:PermittedRole .

ds:hasRulePath  a    owl:ObjectProperty ;
        rdfs:domain  ds:AuthorizationRule ;
        rdfs:range   ds:RulePath .

ds:PermittedRole  a  owl:Class ;
        owl:oneOf  ( ds:CREATE ds:READ ds:UPDATE ds:DELETE ) .
