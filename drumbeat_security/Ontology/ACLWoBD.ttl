@prefix :      <http://www.w3.org/ns/auth/acl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix acl:   <http://www.w3.org/ns/auth/acl#> .
@prefix dc:    <http://purl.org/dc/elements/1.1/> .
@prefix ds:    <https://drumbeat.cs.hut.fi/owl/security.ttl#> .

ds:first  a          owl:ObjectProperty ;
        rdfs:domain  ds:ListNode .

ds:ProtectedResource  a  owl:Class .

[ a                  owl:Restriction ;
  owl:allValuesFrom  rdf:Property ;
  owl:onProperty     ds:first
] .

ds:hasRolePath  a    owl:ObjectProperty ;
        rdfs:domain  ds:ACL ;
        rdfs:range   ds:ListNode .


ds:rest  a           owl:ObjectProperty ;
        rdfs:domain  ds:ListNode ;
        rdfs:range   ds:ListNode .

ds:ListNode  a  owl:Class .


ds:hasClub  a        owl:ObjectProperty ;
        rdfs:domain  ds:ProtectedResource ;
        rdfs:range   ds:Club .


ds:hasProject  a            owl:ObjectProperty ;
        rdfs:domain         ds:ProtectedResource ;
        rdfs:range          ds:Project ;
        rdfs:subPropertyOf  ds:hasClub .

ds:Project  a            owl:Class ;
        rdfs:subClassOf  ds:Club .


ds:Club  a      owl:Class .


ds:hasACLMode  a     owl:ObjectProperty ;
        rdfs:domain  ds:ACL ;
        rdfs:range   acl:Access .

ds:hasACL  a         owl:ObjectProperty ;
        rdfs:domain  ds:ProtectedResource ;
        rdfs:range   ds:ACL .


ds:ACL  a       owl:Class .


